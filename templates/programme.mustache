{{!
    @template customfield_sprogramme/programme
}}
<form class="customfield-sprogramme" data-region="app">
    <table class="programm-table">
        <thead>
            <tr data-region="columns">
            </tr>
        </thead>
        <tbody data-region="rows">
        </tbody>
    </table>
</form>
<div class="d-none" data-region="disciplineform-container">
    {{> customfield_sprogramme/disciplineform }}
</div>
{{#debug}}
    <div class="d-flex pt-5">
        <a href="#" data-toggle="collapse" data-target="#debugc">Debug</a>
        <div id="debugc" class="collapse">
            <pre id="debug">{{{debug}}}</pre>
        </div>
    </div>
{{/debug}}
{{#js}}
require(['customfield_sprogramme/manager'], function(manager) {
    manager.init({{courseid}});
});
{{/js}}