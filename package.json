{
  "name": "scss",
  "version": "1.0.0",
  "description": "scss compiler",
  "main": "index.js",
  "scripts": {
    "compile:sass": "grunt customfield_sprogramme",
    "watch:sass": "grunt customfield_sprogramme --watch",
    "livereload": "livereload -d .",
    "watch:mustache": "nodemon -e mustache -x \"/opt/homebrew/bin/php ../../admin/cli/purge_caches.php\"",
    "start": "npm-run-all --parallel watch:sass watch:mustache livereload"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "livereload": "^0.9.3",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
